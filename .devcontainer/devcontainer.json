{
    "name": "ecpg-playground",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "workspace",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-azuretools.vscode-docker",
                "EditorConfig.EditorConfig",
                "shardulm94.trailing-spaces"
            ]
        }
    },
    "features": {
        "docker-from-docker": {
            "version": "latest",
            "dockerDashComposeVersion": "v2"
        }
    }
}
